{
  "name": "hlen",
  "version": "0.0.2",
  "manifest_version": 3,
  "description": "force hl=en",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://developer.android.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}